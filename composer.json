{
    "name": "gbox/starter-api",
    "description": "A boilerplate API framework based on Slim PHP with JWT Auth out of the box",
    "keywords": ["boilerplate", "php", "api", "slim"],
    "homepage": "https://github.com/bmcminn/starter-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Brandtley McMinn",
            "email": "labs@gbox.name",
            "homepage": "http://brandtley.name/"
        }
    ],
    "require": {
        "php": ">=7.1",
        "filp/whoops": "^2.5",
        "monolog/monolog": "^2.0",
        "opis/database": "^4.1",
        "php-di/php-di": "^6.0",
        "slim/psr7": "^0.5.0",
        "slim/slim": "^4.2",
        "vlucas/phpdotenv": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "scripts": {
        "start": "php -S localhost:3005 -t public"
    }
}
